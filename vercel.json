{
  "framework": "nextjs",
  "routes": [
    {
      "src": "/app(.*)",
      "dest": "/app$1",
      "continue": true
    },
    {
      "src": "/admin(.*)",
      "dest": "/admin$1",
      "continue": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/generate-suggestions",
      "schedule": "0 8 * * *"
    }
  ]
}
